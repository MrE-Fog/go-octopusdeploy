{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://octopus.com/2020/package-reference.json",
    "title": "Package Reference",
    "description": "Represents a package reference",
    "type": "object",
    "properties": {
        "default_value": {
            "$ref": "property_value.json"
        },
        "display_settings": {
            "type": "object",
            "additionalProperties": {
                "type": "string"
            }
        },
        "help_text": {
            "type": "string"
        },
        "id": {
            "description": "unique identifier of a package reference",
            "readOnly": true,
            "type": "string"
        },
        "label": {
            "type": "string"
        },
        "name": {
            "type": "string"
        }
    }
}